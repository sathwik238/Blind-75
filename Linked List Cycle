# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def hasCycle(self, head: Optional[ListNode]) -> bool:

        visited_node = set()

        while(head):
            if head not in visited_node:
                visited_node.add(head)
            else:
                return True
            head = head.next
        return False
        
